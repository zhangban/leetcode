other people
class Solution:
    def twoSum(self, numbers, target):
        dic={}
        for i,number in enumerate(numbers):
            if((target-number) in dic):
                return [dic[target-number]+1,i+1]
            dic[number] = i



190. Reverse Bits
class Solution:
    # @param n, an integer
    # @return an integer
    def reverseBits(self, n):
        
        return int(bin(n)[2:].zfill(32)[::-1],2)
        
class Solution:
    # @param n, an integer
    # @return an integer
    def reverseBits(self, n):
        s = bin(n)[2:]
        s = "0"*(32 - len(s)) + s
        t = s[::-1]
        return int(t,2)

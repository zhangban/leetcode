167. Two Sum II - Input array is sorted
class Solution:
    def twoSum(self, numbers, target):
        # enumerate(numbers, [start=1])
        l=0
        r=len(numbers)-1
        while l < r :
            if numbers[l] + numbers[r]== target:
                return [l+1,r+1]
            elif numbers[l] + numbers[r] < target:
                l += 1
            else:
                r -= 1
            
